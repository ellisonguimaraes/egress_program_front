<MudDialog @ref="MudDialog">
    <DialogContent>
        Dialog Courses
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] MudDialog MudDialog { get; set; }

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
}